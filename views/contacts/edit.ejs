<% include ../header %>
<header>
    <h1>Nodetalk</h1>
    <h4>Edit contact</h4>
</header>
<section>
    <form action="/contacts/<%- id %>?_method=put" method="post">
        <label>Name:</label>
        <input type="text" name="contact[name]" value="<%- contact.name %>">
        <label>E-mail:</label>
        <input type="text" name="contact[email]" value="<%- contact.email %>">
        <button type="submit">Save</button>
    </form>
</section>
<% include ../exit %>
<% include ../footer %>